ZSL
zsl_dict = {
    'A': 'ğŸ¤Ÿ', 'B': 'ğŸ‘', 'C': 'ğŸ¤', 'D': 'ğŸ¤˜', 'E': 'ğŸ––',
    'F': 'ğŸ¤™', 'G': 'âœŒï¸', 'H': 'ğŸ‘Œ', 'I': 'ğŸ‘†', 'J': 'ğŸ‘‡',
    'K': 'ğŸ–ï¸', 'L': 'ğŸ‘‹', 'M': 'ğŸ¤š', 'N': 'ğŸ––', 'O': 'ğŸ‘Œ',
    'P': 'â˜ï¸', 'Q': 'âœŠ', 'R': 'ğŸ‘Š', 'S': 'âœ‹', 'T': 'ğŸ¤›',
    'U': 'ğŸ¤œ', 'V': 'ğŸ’ª', 'W': 'âœŒï¸', 'X': 'ğŸ‘', 'Y': 'ğŸ¤',
    'Z': 'ğŸ¤Ÿ', ' ': ' '  # Space remains unchanged
}

def text_to_zsl(text):
    text = text.upper()  # Convert input text to uppercase
    zsl_text = ''
    for char in text:
        # Get ZSL gesture for each character or keep unchanged if not found
        asl_text += asl_dict.get(char, char) + ' '
    return asl_text

# Example usage
input_text = "HELLO WORLD"
zsl_output = text_to_zsl(input_text)
print(zsl_output)
